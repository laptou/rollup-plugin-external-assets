{
  "name": "rollup-plugin-external-assets",
  "version": "3.0.1",
  "description": "A rollup plugin to make assets external but include them in the output.",
  "main": "dist/cjs/index.js",
  "module": "dist/es/index.js",
  "sideEffects": false,
  "files": [
    "dist/**/*"
  ],
  "scripts": {
    "test": "rimraf tests/coverage && cross-env ROLLUP_WARNINGS=false jest",
    "test:warn": "rimraf tests/coverage && cross-env ROLLUP_WARNINGS=true jest",
    "test:coverage": "rimraf tests/coverage && cross-env ROLLUP_WARNINGS=false jest --coverage",
    "coverage": "codecov",
    "clean": "rimraf dist tests/coverage",
    "build": "rimraf dist && rollup -c",
    "prepare": "husky install && cd tests/fixtures && npm install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/recursive-beast/rollup-plugin-external-assets.git"
  },
  "keywords": [
    "rollup-plugin",
    "assets",
    "import",
    "external"
  ],
  "author": {
    "name": "Soufiane Yakoubi",
    "email": "splentercell.1997@gmail.com"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/recursive-beast/rollup-plugin-external-assets/issues"
  },
  "homepage": "https://github.com/recursive-beast/rollup-plugin-external-assets#readme",
  "devDependencies": {
    "@commitlint/cli": "^16.2.1",
    "@commitlint/config-conventional": "^16.2.1",
    "@rollup/plugin-alias": "^3.1.9",
    "@rollup/plugin-node-resolve": "^13.1.3",
    "@semantic-release/changelog": "^6.0.1",
    "@semantic-release/git": "^10.0.1",
    "@types/fs-extra": "^9.0.13",
    "@types/jest": "^27.4.0",
    "codecov": "^3.8.2",
    "cross-env": "^7.0.3",
    "fs-extra": "^10.0.0",
    "husky": "^7.0.4",
    "jest": "^27.5.1",
    "rimraf": "^3.0.2",
    "rollup": "^2.67.3",
    "rollup-plugin-ts": "^2.0.5",
    "semantic-release": "^19.0.2",
    "ts-jest": "^27.1.3",
    "ts-transform-default-export": "^1.0.3",
    "typescript": "^4.5.5"
  },
  "dependencies": {
    "@rollup/pluginutils": "^4.1.2",
    "recast": "^0.20.5"
  }
}
