{
  "name": "rollup-plugin-external-assets",
  "version": "2.2.1",
  "description": "A rollup plugin to make assets external but include them in the output.",
  "main": "dist/cjs/index.js",
  "module": "dist/es/index.js",
  "sideEffects": false,
  "files": [
    "dist/**/*"
  ],
  "scripts": {
    "test": "del-cli tests/coverage && cross-env ROLLUP_WARNINGS=false jest",
    "test:warn": "del-cli tests/coverage && cross-env ROLLUP_WARNINGS=true jest",
    "test:update": "npm run test -- -u",
    "test:update:warn": "npm run test:warn -- -u",
    "coverage": "codecov",
    "clean": "del-cli dist tests/coverage",
    "build": "del-cli dist && rollup -c",
    "prepare": "cd tests/fixtures && npm install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/soufyakoub/rollup-plugin-external-assets.git"
  },
  "keywords": [
    "rollup-plugin",
    "assets",
    "import",
    "external"
  ],
  "author": {
    "name": "Soufiane Yakoubi",
    "email": "splentercell.1997@gmail.com"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/soufyakoub/rollup-plugin-external-assets/issues"
  },
  "homepage": "https://github.com/soufyakoub/rollup-plugin-external-assets#readme",
  "devDependencies": {
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@rollup/plugin-alias": "^3.1.2",
    "@rollup/plugin-node-resolve": "^11.2.0",
    "@semantic-release/changelog": "^5.0.1",
    "@semantic-release/git": "^9.0.0",
    "@types/jest": "^26.0.22",
    "@wessberg/rollup-plugin-ts": "^1.3.8",
    "codecov": "^3.8.1",
    "cross-env": "^7.0.3",
    "del": "^6.0.0",
    "del-cli": "^3.0.1",
    "husky": "^5.0.9",
    "jest": "^26.6.3",
    "rollup": "^2.39.0",
    "semantic-release": "^17.3.9",
    "ts-jest": "^26.5.4",
    "ts-transform-default-export": "^1.0.3",
    "typescript": "^4.1.5"
  },
  "dependencies": {
    "@rollup/pluginutils": "^3.1.0",
    "recast": "^0.20.4"
  }
}
